<div class="title">
  <h1>All Contacts</h1>
</div>


<% @@rolodex.contacts.each do |contact| %>
<p>ID #: <%= contact.id %>
<br />
Name: <a href="/contacts/<%= contact.id %>"><%= contact.first_name %> <%= contact.last_name %></a>
<br />
Email: <%= contact.email %>
<br /></p>
Note: <%= contact.note %>
<br />
<hr width="40%" align=left>
<% end %> 